{
    "name": "modera/security-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "description": "Provides database aware implementation of Security model + some other perks",
    "authors": [
        {
            "name": "Sergei Vizel",
            "email": "sergei.vizel@modera.org"
        },
        {
            "name": "Sergei Lissovski",
            "email": "sergei.lissovski@modera.org",
            "homepage": "http://sergei.lissovski.org/"
        }
    ],
    "require": {
        "php": ">=5.6",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "symfony/swiftmailer-bundle": "^2.6|^3.0",
        "symfony/validator": "^3.4|^4.0",
        "symfony/security-bundle": "^3.4|^4.0",
        "symfony/framework-bundle": "^3.4|^4.0",
        "modera/foundation-bundle": "~3.1|~4.0",
        "modera/translations-bundle": "~3.1|~4.0",
        "sergeil/extjsintegration-bundle": "^2.0"
    },
    "require-dev": {
        "phake/phake": "^2.0",
        "phpunit/phpunit": "^5.5",
        "modera/foundation-bundle": "~3.1",
        "modera/languages-bundle": "~3.1"
    },
    "autoload": {
        "psr-0": { "Modera\\SecurityBundle": "" }
    },
    "target-dir": "Modera/SecurityBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    }
}
